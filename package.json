{
  "name": "@uprise-fin/dndn-svelte-ui",
  "version": "0.1.49",
  "description": "DNDN UI Components",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "src/lib/**",
    "src/stories/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uprise-fin/dndn-svelte-ui.git"
  },
  "author": "uprise-fin",
  "license": "MIT",
  "keywords": [
    "uprise",
    "ui",
    "design",
    "system",
    "svelte",
    "storybook"
  ],
  "scripts": {
    "svg": "node svg/assetsToJs.js",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "pnpm svg && storybook dev -p 6006",
    "build-storybook": "pnpm svg && storybook build"
  },
  "peerDependencies": {
    "sass": "^1.69.5"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.4.2",
    "@storybook/addon-interactions": "^8.4.2",
    "@storybook/addon-links": "^8.4.2",
    "@storybook/addon-mdx-gfm": "^8.4.2",
    "@storybook/blocks": "^8.4.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/svelte": "^8.4.2",
    "@storybook/sveltekit": "^8.4.2",
    "@storybook/test": "^8.4.2",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-svelte": "^2.30.0",
    "pretendard": "^1.3.9",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "storybook": "^8.4.2",
    "style-loader": "^3.3.3",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.5.3",
    "vitest": "^0.34.0"
  },
  "type": "module"
}